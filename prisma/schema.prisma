generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transaction {
  id        Int      @id @default(autoincrement())
  userId    String
  amount    Float
  type      String
  category  String
  recipient String
  createdAt DateTime @default(now())
}
